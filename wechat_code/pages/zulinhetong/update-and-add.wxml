
<view class="update-container">
<view class="content">
<view class="form-box">
<view class="item-box">
<view class="item-label">房屋名称</view>
<input  class="item-input"
type="text"  disabled="{{ro.fangwumingcheng==true}}"  placeholder="房屋名称" model:value="{{fangwumingcheng}}" />
</view>
<view class="item-box">
<view class="item-label">房屋地址</view>
<input  class="item-input"
type="text"  disabled="{{ro.fangwudizhi==true}}"  placeholder="房屋地址" model:value="{{fangwudizhi}}" />
</view>
<view class="item-box">
<view class="item-label">房东账号</view>
<input  class="item-input"
type="text"  disabled="{{ro.fangdongzhanghao==true}}"  placeholder="房东账号" model:value="{{fangdongzhanghao}}" />
</view>
<view class="item-box">
<view class="item-label">房东姓名</view>
<input  class="item-input"
type="text"  disabled="{{ro.fangdongxingming==true}}"  placeholder="房东姓名" model:value="{{fangdongxingming}}" />
</view>
<view class="item-box">
<view class="item-label">用户账号</view>
<input  class="item-input"
type="text"  disabled="{{ro.yonghuzhanghao==true}}"  placeholder="用户账号" model:value="{{yonghuzhanghao}}" />
</view>
<view class="item-box">
<view class="item-label">用户姓名</view>
<input  class="item-input"
type="text"  disabled="{{ro.yonghuxingming==true}}"  placeholder="用户姓名" model:value="{{yonghuxingming}}" />
</view>
<view class="item-box">
<view class="item-label">手机号码</view>
<input  class="item-input"
type="text"  disabled="{{ro.shoujihaoma==true}}"  placeholder="手机号码" model:value="{{shoujihaoma}}" />
</view>
<view class="item-box">
<view class="item-label">备注</view>
<input  class="item-input"
type="text"  disabled="{{ro.beizhu==true}}"  placeholder="备注" model:value="{{beizhu}}" />
</view>
<view class="item-box"  >
<view class="item-label">签订时间</view>
<view class="down-item-picker"  bindtap="onqiandingshijianTap">
<view class="item-input" >         {{qiandingshijian}}</view>
</view>
</view>
<com-date isShowPicker="{{showqiandingshijian}}" bind:dateHandler="qiandingshijianTap"></com-date>



<com-edit></com-edit>
</view>


<view class="btn-box">



<button     class="submit-btn" bindtap="submit">提交</button>
</view>


</view>

</view>